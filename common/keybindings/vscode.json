[
    {
        "key": "shift+alt+a",
        "when": "editorTextFocus && !editorReadonly",
        "action": "TOGGLE_COMMENT_BLOCK"
    },
    {
        "key": "ctrl+c",
        "when": "editorTextFocus",
        "action": "COPY"
    },
    {
        "key": "ctrl+/",
        "when": "editorTextFocus && !editorReadonly",
        "action": "TOGGLE_COMMENT_LINE"
    },
    {
        "key": "f12",
        "when": "editorHasDefinitionProvider && editorTextFocus",
        "action": "GO_TO_DECLARATION"
    },
    {
        "key": "f2",
        "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly",
        "action": "RENAME"
    },
    {
        "key": "f5",
        "when": "inDebugMode",
        "action": "RESUME"
    },
    {
        "key": "ctrl+f5",
        "when": "!inDebugMode",
        "action": "DEBUG"
    },
    {
        "key": "f5",
        "when": "!inDebugMode",
        "action": "BUILD_AND_RUN"
    },
    {
        "key": "shift+f11",
        "when": "inDebugMode",
        "action": "STEP_OUT"
    },
    {
        "key": "f10",
        "when": "inDebugMode",
        "action": "STEP_OVER"
    },
    {
        "key": "alt+left",
        "action": "NAVIGATE_TAB_BACKWARDS"
    },
    {
        "key": "alt+right",
        "action": "NAVIGATE_TAB_FORWARDS"
    },
    {
        "key": "ctrl+shift+b",
        "action": "BUILD"
    },
    {
        "key": "f11",
        "when": "inDebugMode",
        "action": "STEP_INTO"
    }
]